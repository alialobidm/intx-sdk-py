# Copy to .env and fill in values

# Separate variables (recommended)
INTX_ACCESS_KEY=your_access_key_here
INTX_PASSPHRASE=your_passphrase_here
INTX_SIGNING_KEY=your_signing_key_here

# Optional: environment selection (defaults to production)
INTX_ENVIRONMENT=SANDBOX

# Optional: default portfolio ID (reduces copy/paste in examples)
# NOTE: Use portfolio_id (e.g., "1w98n1ex-1-0"), NOT portfolio_uuid
INTX_PORTFOLIO_ID=your_portfolio_id_here